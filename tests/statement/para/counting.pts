$Test of para loop

var int i;


para (a de 0..5) faca
  i += 1;



mostra i; $ expect 5

$ with block
i = 0;

para (a de 0..10) faca{
    i += 1;
}
mostra i; $ expect 10

$ with inc
i = 0;
para (c de 0..10 inc 2) faca
  i += 1;

mostra i; $ expect 5


$ counting backwards
i = 0;
para (c de 10..0) faca
  i += 1;
mostra i; $ expect 10


$ with var params
i = 0;
var int ini = 0;
var int fim = 10;
var int passo = 3;


para (c de ini..fim inc passo) faca {
  i+=1;
}

mostra i; $ expect 4
