

classe Animal

     nome:texto
     idade:int

    func constructor(nome:texto,idade:int)
        eu.nome = nome
        eu.idade = idade
    fim
    
    func falar(palavras : texto): texto
        mostra palavras
        retorna palavras
    fim

fim


classe Humano < Animal  
    
    func constructor(nome:texto,idade:int)
        super(nome,idade)
    fim

    func meu_nome():texto
        retorna super.nome
    fim

fim

humano : Humano = Humano("John",21)
humano.falar("Eu sou um humano") # Eu sou humano

mostra humano.nome # expect John

mostra humano.idade # expect 21

mostra humano.meu_nome()
