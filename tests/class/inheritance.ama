
classe Animal

    var nome:Texto
    var idade:int
    
    func falar(palavras : Texto): Texto
        mostra palavras
        retorna palavras
    fim

fim


classe Humano < Animal  fim

var humano : Humano = Humano()
humano.falar("Eu sou um humano")

humano.nome = "John"
mostra humano.nome

humano.idade = 21
mostra humano.idade
