
classe Animal

     nome:texto
     idade:int
    
    func falar(palavras : texto): texto
        mostra palavras
        retorna palavras
    fim

fim


classe Humano < Animal  fim

humano : Humano = Humano()
humano.falar("Eu sou um humano")

humano.nome = "John"
mostra humano.nome

humano.idade = 21
mostra humano.idade
