classe Acumulador
    
     contador : int 
    func incrementa(diff : int):int
        contador += diff
        retorna contador
    fim

    func get_contador():int
        retorna contador
    fim

fim



acum : Acumulador = Acumulador()
acum.contador = 6
mostra acum.contador # expect 6

acum.contador = "str".cmp()
mostra acum.contador # expect 3
