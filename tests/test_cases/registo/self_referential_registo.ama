registo No
    valor : int
    proximo : No
fim


registo ListaLigada
    cabeca : No
fim


met ListaLigada::ad_no(alvo, valor : int)
    novo : No = No(valor, nulo)

    se alvo.cabeca == nulo entao
        alvo.cabeca = novo
    senao
        ultimo : No = alvo.cabeca
        enquanto ultimo.proximo != nulo faca
            ultimo = ultimo.proximo
        fim
        ultimo.proximo = novo
    fim
fim

met ListaLigada::imprimir(alvo)
    node : No = alvo.cabeca
    enquanto node != nulo faca
        mostra node.valor
        node = node.proximo
    fim
fim

l : ListaLigada = ListaLigada(nulo)
l.ad_no(1)
l.ad_no(2)
l.ad_no(3)
l.imprimir()
#[output]: 1 2 3
