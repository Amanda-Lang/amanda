# Class with empty constructor


#uma classe pra contar loool
classe Acumulador
    
    #Fields must be declared explicitly
     contador : int 
    #Can have methods
    func incrementa(diff : int)
        contador += diff
    fim

    func get_contador():int
        retorna contador
    fim

fim



