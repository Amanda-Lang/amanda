
func doubler(l1 : [] int) : [] int
    
    para i de 0..tamanho(l1) faca
        l1[i] *= 2
    fim
    retorna l1
fim


func fmt_lista(l1:[] int,sep : texto)
    
    resultado : texto
    para i de 0..tamanho(l1) faca
        valor : texto = converte(l1[i],texto)
        se i == tamanho(l1) - 1 entao
            resultado += valor
        senao
            resultado += valor + sep
        fim
    fim
    mostra resultado
fim


l1 : []int = lista(int,3)
l1[0] = 2
l1[1] = 3
l1[2] = 5

l1 = doubler(l1)

fmt_lista(l1,",")

#[output]:4,6,10