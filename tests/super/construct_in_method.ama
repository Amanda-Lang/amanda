
classe Animal

    var nome:Texto
    var idade:int

    func constructor(nome:Texto,idade:int)
        eu.nome = nome
        eu.idade = idade
    fim
    
    func falar(palavras : Texto): Texto
        mostra palavras
        retorna palavras
    fim

fim


