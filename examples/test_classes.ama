#Quick file to test classes


#uma classe pra contar loool
classe Acumulador
    
    #Fields must be declared explicitly
    var contador : int 


    #Can have methods
    func incrementa(diff : int):int
        contador += diff
        retorna contador
    fim

    func get_contador():int
        retorna contador
    fim
fim



var acum : Acumulador = Acumulador()


acum.incrementa(4)
mostra acum.get_contador()
acum.incrementa(5)
mostra acum.get_contador()
acum.incrementa(1)
mostra acum.get_contador()
mostra acum.contador

